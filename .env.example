# API Keys
#ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
#OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Platform
#GCP_PROJECT_ID=your-gcp-project-id
#VERTEX_AI_LOCATION=us-central1
#GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Redis Cache (Upstash)
#UPSTASH_REDIS_URL=https://your-redis-instance.upstash.io
#UPSTASH_REDIS_TOKEN=your_upstash_token_here

# Cost Management
#DAILY_BUDGET=50.0
#ALERT_THRESHOLD=0.8

# Feature Flags
ENABLE_VOICE_CACHE=true
ENABLE_SMART_ROUTING=true
ENABLE_COST_TRACKING=true

# Application Settings
PORT=8000
ENVIRONMENT=development
LOG_LEVEL=info

# Frontend URLs (for CORS)
FRONTEND_URL=http://localhost:3000
ALLOWED_ORIGINS=http://localhost:3000,https://fantasyfootballneuron.com

# Optional: Cloud Storage for L2 Cache
GCS_BUCKET_NAME=ffn-audio-cache
# Or AWS S3
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# S3_BUCKET_NAME=ffn-audio-cache

# Optional: Monitoring
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Optional: Analytics
#GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
#MIXPANEL_TOKEN=your_mixpanel_token

# Development Settings
DEBUG=false
RELOAD=true
